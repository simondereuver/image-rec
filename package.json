{
  "name": "image-rec-proj",
  "version": "1.0.0",
  "description": "Initial package file",
  "dependencies": {
    "express": "4.18.2",
    "sqlite3": "5.1.6"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "eslint": "^8.46.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "htmlhint": "^1.1.4",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "supertest": "^6.3.3"
  },
  "scripts": {
    "start": "node index.js",
    "test": "mocha && nyc report --reporter=text-lcov > coverage.lcov && nyc report --reporter=html",
    "lint-server": "eslint server --config lint-config/.eslintrc.js",
    "lint-client": "eslint client --config lint-config/.eslintrc.js",
    "lint-test": "eslint test --config lint-config/.eslintrc.test.js",
    "lint-css": "stylelint \"Client/**/*.css\" --config lint-config/.stylelintrc.js",
    "lint-html": "htmlhint \"Client/**/*.html\" --config lint-config/.htmlhintrc",
    "lint": "npm run lint-server && npm run lint-client && npm run lint-css && npm run lint-html && npm run lint-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simondereuver/image-rec.git"
  },
  "author": "Simon de Reuver",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simondereuver/image-rec/issues"
  },
  "homepage": "https://github.com/simondereuver/image-rec#readme"
}
